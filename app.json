{
  "expo": {
    "name": "firstProject",
    "slug": "firstProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "NaughtySailor/assets/logo/logo1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/43341.png",
      "resizeMode": "contain",
      "backgroundColor": "#E2B5B5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/43341.png",
        "backgroundColor": "#2b2b2b"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share features of your app."
        }
      ]
    ]
    
  }
}
